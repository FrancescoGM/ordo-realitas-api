{
  "name": "ordo-realitas-api",
  "version": "1.0.0",
  "description": "The ordo realitas api",
  "private": true,
  "license": "MIT",
  "scripts": {
    "lint:fix": "npm run lint:fix:eslint && npm run lint:fix:prettier",
    "lint:fix:eslint": "eslint --fix",
    "lint:fix:prettier": "prettier --write '**/*.{json,js,ts}'",
    "prepare": "husky install",
    "commit": "cz",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@prisma/client": "3.9.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "prisma": "^3.9.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "homepage": "https://ordo-realitas.com.br",
  "repository": {
    "type": "git",
    "url": "https://github.com/FrancescoGM/ordo-realitas-api.git"
  },
  "author": "Francesco Gonzalez Muller <francescogmuller@hotmail.com> (https://github.com/FrancescoGM)",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
